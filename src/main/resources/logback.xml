<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>
				%highlight(%d{HH:mm:ss.SSS}) [%thread] %highlight(%level) %highlight(%logger{100}) - %highlight(%msg%n)
			</Pattern>
		</layout>
	</appender>
	<logger name="fr.aberwag" level="DEBUG" />
	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>